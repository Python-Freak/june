struct Node {
    data: i64
    next: Node?
}

fun main() {
    mut node3 = new Node(data: 3, next: none)
    let node2 = new Node(data: 2, next: node3)
    let node1 = new Node(data: 1, next: node2)
    node3.next = node1

    mut curr: Node? = node2
    let end = curr

    println(curr.data)

    while curr.next != end {
        curr = curr.next
        println(curr.data)
    }
}