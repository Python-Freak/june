enum State {
    None
    StateId(i64)
    Duration { from: i64, to: i64 }
}

fun main() {
    let x = State::None
    let y = State::StateId(4)
    let z = State::Duration(from: 1, to: 10)

    match x {
        State::None => println("None")
    }

    match y {
        State::StateId(x) => println(x)
    }

    match z {
        State::Duration(from, to) => {
            println(from)
            println(to)
        }
    }
}