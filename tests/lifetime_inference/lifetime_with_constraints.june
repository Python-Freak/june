// output: whatever

struct Stats {
    age: i64
}

struct Employee {
    name: string,
    stats: Stats,
}

fun helper(mut employee: Employee, stats: Stats) [employee == stats] {
    employee.stats = stats
}

fun main() {
    mut employee = new Employee(name: "Sophia", stats: new Stats(age: 22))

    helper(employee, new Stats(age: 33))

    println(employee.stats.age)
}