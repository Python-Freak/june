extern type FILE;

extern "C" fun fopen(filename: c_string, mode: c_string) -> FILE?
extern "C" fun fclose(file: FILE?) -> c_int;
extern "C" fun fgetc(file: FILE?) -> c_int;

fun main() {
    let file = fopen("test_data/alphabet.txt", "rb");
    if file != none {
        let x = fgetc(file)
        println(x)
    }
    fclose(file)
}
