struct Node<T> {
    data: T
    next: raw Node<T>?
}

fun main() {
    let node3 = new raw Node(data: 3, next: None)
    let node2 = new raw Node(data: 2, next: node3)
    let node1 = new raw Node(data: 1, next: node2)
    node3.next = node1

    let curr = node2
    let end = curr

    while curr.next != end {
        println("{curr.data}")
        curr = curr.next
    }
}